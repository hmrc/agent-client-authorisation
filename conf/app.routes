# Agent routes
POST     /agencies/:arn/invitations                     @uk.gov.hmrc.agentclientauthorisation.controllers.InvitationsController.createInvitation($arn: uk.gov.hmrc.agentclientauthorisation.model.Arn)
GET      /agencies/:arn/invitations/sent                @uk.gov.hmrc.agentclientauthorisation.controllers.InvitationsController.getSentInvitations($arn: uk.gov.hmrc.agentclientauthorisation.model.Arn)
GET      /agencies/:arn/invitations/sent/:invitation    @uk.gov.hmrc.agentclientauthorisation.controllers.InvitationsController.getSentInvitation($arn: uk.gov.hmrc.agentclientauthorisation.model.Arn, invitation: String)